<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="net/n2oapp/framework/autotest/action/modal/ref_modal/test.json" operation="findAll"/>
    </list>
    <count>
        <test file="net/n2oapp/framework/autotest/action/modal/ref_modal/test.json" operation="count"/>
    </count>
    <unique filters="id">
        <test file="net/n2oapp/framework/autotest/action/modal/ref_modal/test.json" operation="findOne"/>
    </unique>
    <fields>
        <field id="id" domain="integer">
            <select/>
            <filters>
                <eq filter-id="id">id:eq:id</eq>
            </filters>
        </field>
        <field id="name">
            <select/>
            <filters>
                <eq filter-id="name">name:eq:name</eq>
            </filters>
        </field>
        <field id="type">
            <select/>
            <filters>
                <eq filter-id="type">type:eq:type</eq>
            </filters>
        </field>
    </fields>
</query>
