<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="net/n2oapp/framework/autotest/widget/form/filter/test.json" operation="findAll"/>
    </list>
    <count>
        <test file="net/n2oapp/framework/autotest/widget/form/filter/test.json" operation="count"/>
    </count>
    <unique filters="id">
        <test file="net/n2oapp/framework/autotest/widget/form/filter/test.json" operation="findOne"/>
    </unique>
    <fields>
        <field id="id" domain="integer">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="period.id">
            <select/>
            <filters>
                <eq filter-id="period.id"/>
            </filters>
        </field>
    </fields>
</query>
