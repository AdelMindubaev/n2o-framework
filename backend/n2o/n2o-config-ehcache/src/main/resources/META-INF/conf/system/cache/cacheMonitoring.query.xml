<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>cacheConfig</object-id>
    <name>cacheMonitoring</name>
    <execution>
        <n2o:java-criteria bean-name="cacheStatisticService"
                           criteria-class="net.n2oapp.framework.config.ehcache.monitoring.api.CacheCriteria"/>
    </execution>

    <fields>
        <field>
            <id>id</id>
        </field>

        <field>
            <id>name</id>
            <domain>string</domain>
            <display/>
            <sorting/>
        </field>

        <field>
            <id>fullness</id>
            <domain>numeric</domain>
            <display/>
            <sorting/>
        </field>

        <field>
            <id>currentSize</id>
            <domain>string</domain>
            <display/>
        </field>
        
        <field>
            <id>fullnessState</id>
            <domain>integer</domain>
            <display/>
        </field>

        <field>
            <id>hits</id>
            <domain>integer</domain>
            <display/>
        </field>

        <field>
            <id>miss</id>
            <domain>integer</domain>
            <display/>
        </field>

        <field>
            <id>usefulness</id>
            <domain>numeric</domain>
            <display/>
            <sorting/>
        </field>

        <field>
            <id>usefulnessState</id>
            <domain>integer</domain>
            <display/>
        </field>

        <field>
            <id>hasStatistic</id>
            <domain>boolean</domain>
            <display/>
        </field>

        <field>
            <id>hasTimeEvict</id>
            <domain>boolean</domain>
            <display/>
        </field>
    </fields>
</query>
