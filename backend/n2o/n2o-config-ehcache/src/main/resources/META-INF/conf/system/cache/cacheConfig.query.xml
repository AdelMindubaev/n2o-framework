<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>cacheConfig</object-id>
    <name>cacheConfig</name>
    <execution>
        <n2o:java-criteria bean-name="cacheConfigService"
                           criteria-class="net.n2oapp.framework.config.ehcache.monitoring.api.CacheCriteria"/>
    </execution>

    <fields>
        <field>
            <id>id</id>
        </field>

        <field>
            <id>name</id>
            <domain>string</domain>
            <display/>
            <sorting/>
            <filters>
                <filter type="eq" filter-field="name"/>
            </filters>
        </field>

        <field>
            <id>timeToLive</id>
            <domain>integer</domain>
            <display/>
        </field>
        <field>
            <id>timeToIdle</id>
            <domain>integer</domain>
            <display/>
        </field>
        <field>
            <id>maxEntriesLocalHeap</id>
            <domain>integer</domain>
            <display/>
        </field>
        <field>
            <id>maxBytesLocalHeap</id>
            <domain>integer</domain>
            <display/>
        </field>
        <field>
            <id>maxEntriesLocalDisk</id>
            <domain>integer</domain>
            <display/>
        </field>
        <field>
            <id>maxBytesLocalDisk</id>
            <domain>integer</domain>
            <display/>
        </field>
        <field>
            <id>percentOfTotal</id>
            <domain>integer</domain>
            <display/>
        </field>
        <field>
            <id>isOverflowToOffHeap</id>
            <domain>boolean</domain>
            <display/>
        </field>
        <field>
            <id>isFrozen</id>
            <domain>boolean</domain>
            <display/>
        </field>

    </fields>
</query>
