<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <name>Использование памяти</name>
    <execution>
        <n2o:java-criteria bean-name="n2oMemoryService"/>
    </execution>

    <fields>
        <field>
            <id>usedMemory</id>
            <domain>long</domain>
            <display/>
        </field>
        <field>
            <id>usedMemoryPercent</id>
            <domain>short</domain>
            <display/>
        </field>
    </fields>
</query>
