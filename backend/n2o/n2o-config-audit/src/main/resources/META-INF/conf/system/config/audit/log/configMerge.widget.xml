<form xmlns="http://n2oapp.net/framework/config/schema/n2o-widget-3.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0">
    <query-id>configAudit</query-id>
    <name>Слияние с веткой</name>
    <modal-width>500px</modal-width>
    <fs:field-set>
        <ctrl:input-select id="branch" label="Название ветки">
            <ctrl:query query-id="configBranch" label-field-id="name" value-field-id="id"/>
            <ctrl:description>Все изменения этой ветки будут слиты в текущий стенд</ctrl:description>
        </ctrl:input-select>
    </fs:field-set>
</form>
