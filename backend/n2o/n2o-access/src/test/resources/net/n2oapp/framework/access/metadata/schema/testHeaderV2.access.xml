<access xmlns="http://n2oapp.net/framework/config/schema/access-2.0">

    <user username="user">
        <page-access page-id="testHeaderAccessTransformer"/>
        <object-access object-id="testObjectAccessTransformer"/>
    </user>
    <role id="role">
        <page-access page-id="testHeaderAccessTransformer"/>
        <url-access pattern="/test/*"/>
    </role>
    <role id="admin">
        <page-access page-id="testHeaderAccessTransformer"/>
    </role>
    <permission id="permission">
        <page-access page-id="test"/>
        <object-access object-id="test" operations="create"/>
    </permission>
    <anonymous>
        <object-access object-id="testObjectAccessTransformer"/>
    </anonymous>

</access>
