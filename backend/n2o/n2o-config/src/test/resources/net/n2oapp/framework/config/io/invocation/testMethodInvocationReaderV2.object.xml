    <name>testMethodInvocationReaderV2</name>
    <entity-class>java.lang.Integer</entity-class>

    <actions>
        <action id="create">
            <invocation>
                <java:java class="test" method="test">
                    <java:arguments>
                        <java:argument class="test" name="first" type="primitive"/>
