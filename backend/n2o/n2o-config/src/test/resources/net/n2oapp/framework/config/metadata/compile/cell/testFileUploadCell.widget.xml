<?xml version='1.0' encoding='UTF-8'?>
<table xmlns="http://n2oapp.net/framework/config/schema/n2o-widget-4.0">
    <columns>
        <column text-field-id="test1">
            <file-upload/>
        </column>
        <column text-field-id="test2">
            <file-upload multi="true" ajax="false"
                         upload-url="/uploadDoc" delete-url="/deleteDoc"
                         value-field-id="testId" label-field-id="testName"
                         message-field-id="testMessage" url-field-id="/downloadDoc"
                         request-param="testFile" show-size="false"
                         accept="jpg,doc" label="label"
                         delete-icon="deleteIcon" upload-icon="uploadIcon"
                         class="testClass" visible="true" style="margin-left:10px;width:300px" src="src"/>
        </column>
    </columns>
</table>
