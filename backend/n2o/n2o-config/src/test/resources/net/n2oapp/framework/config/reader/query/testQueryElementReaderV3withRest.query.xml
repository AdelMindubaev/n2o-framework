<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>blank</object-id>
    <name>test</name>
    <execution>
        <n2o:rest>
            <n2o:query>query</n2o:query>
            <n2o:query-by-id>query-by-id</n2o:query-by-id>
            <n2o:response-mapping collection="collection" count="count"/>
            <n2o:proxy host="host" port="2222"/>
            <n2o:date-format>dd.MM.yyyy</n2o:date-format>
            <n2o:error-mapping>
                <n2o:message param-name="error-message"/>
                <n2o:detailed-message param-name="error-detail"/>
                <n2o:stack-trace param-name="error-stack-trace"/>
            </n2o:error-mapping>
        </n2o:rest>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
            <name>test</name>
            <display/>
        </field>
    </fields>
</query>
