<form xmlns="http://n2oapp.net/framework/config/schema/n2o-widget-4.0">
    <fields>
        <checkbox-group id="gender" inline="false"  css-class="n2o" visible="true" label="Label" query-id="testSelect" label-field-id="name">
            <pre-filters>
                <notEq field-id="ref" value="`someField`"/>
                <contains field-id="country">
                    <value>1</value>
                    <value>2</value>
                    <value>3</value>
                </contains>
            </pre-filters>
        </checkbox-group>

        <checkbox-group id="inn" format="{name}  {inn}" />
        <checkbox-group id="pass" format="password" />
    </fields>
</form>
