<page xmlns="http://n2oapp.net/framework/config/schema/page-2.0">
    <regions>
        <none place="test">
            <form id="main">
                <fields>
                    <input-text id="name" default-value="#{test}"/>
                    <select id="gender">
                        <default-value id="#{test}"/>
                        <options>
                            <option>{"id":1}</option>
                            <option>{"id":2}</option>
                        </options>
                    </select>
                    <date-interval id="birthday">
                        <default-value end="11.11.2018" begin="01.11.2018"/>
                    </date-interval>
                </fields>
            </form>
        </none>
    </regions>
</page>