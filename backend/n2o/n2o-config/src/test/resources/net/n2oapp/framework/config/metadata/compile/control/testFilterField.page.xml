<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-2.0">
    <table query-id="testFilterField">
        <columns>
            <column text-field-id="id"/>
            <column text-field-id="price"/>
            <column text-field-id="year"/>
        </columns>

        <filters>
            <row>
                <input-text id="minPrice"/>
                <input-text id="maxPrice" filter-id="maxP"/>
                <date-interval id="year" begin-filter-id="startDate" end-filter-id="endDate"/>
            </row>
        </filters>
    </table>
</simple-page>
