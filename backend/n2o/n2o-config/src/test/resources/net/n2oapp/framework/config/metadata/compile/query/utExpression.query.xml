<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0"
       name="utExpression">
    <list>
        <sql>test</sql>
    </list>
    <count>
        <sql>test</sql>
    </count>

    <fields>
        <field id="f1" domain="integer"/>
        <field id="manual" domain="integer">
            <expression>test</expression>
            <select mapping="map">_:expression_</select>
            <sorting mapping="map">_:expression_</sorting>
            <filters>
                <eq filter-id="id" mapping="map">_:expression_</eq>
            </filters>
        </field>
        <field id="auto" domain="integer" name="AUTO">
            <expression>test</expression>
            <select/>
            <sorting/>
            <filters>
                <eq filter-id="test"/>
                <like filter-id="testLike"/>
                <in filter-id="testIn"/>
            </filters>
        </field>
        <field id="testFilter">
            <filters>
                <eq required="true"/>
                <in/>
            </filters>
        </field>
        <field id="withEmptySelect">
            <select/>
        </field>
    </fields>
</query>
