<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-1.0"
        xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-invocations-2.0">
    <name>utObject</name>

    <actions>
        <action id="create">
            <invocation>
                <n2o:sql>select 1</n2o:sql>
            </invocation>
            <in-parameters>
                <param name="in1" mapping="[]" domain="boolean"/>
                <param name="in2" mapping="[]" domain="string"/>
            </in-parameters>
            <out-parameters>
                <param name="out" domain="boolean" mapping="[]"/>
            </out-parameters>
        </action>
        <action id="delete">
            <invocation>
                <n2o:sql>select 1</n2o:sql>
            </invocation>
            <in-parameters>
                <param name="a" mapping="a" domain="boolean"/>
                <param name="b" mapping="b" domain="string"/>
            </in-parameters>
        </action>
    </actions>

</object>
