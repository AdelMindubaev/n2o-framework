<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list filters="id, nameLike"/>
    <unique filters="id, codeEq, codeEq2"/>
    <count filters="nameEq"/>

    <fields>
        <field id="id">
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="name">
            <filters>
                <eq filter-id="nameEq"/>
                <like filter-id="nameLike"/>
            </filters>
        </field>
        <field id="code">
            <filters>
                <eq filter-id="codeEq"/>
            </filters>
        </field>
    </fields>
</query>
