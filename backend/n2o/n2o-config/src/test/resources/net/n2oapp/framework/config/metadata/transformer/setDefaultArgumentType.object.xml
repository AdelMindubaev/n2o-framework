<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-4.0" name="setDefaultArgumentType">

    <operations>
        <operation id="testId">
            <invocation>
                <java class="net.TestClass" method="testMethod">
                    <arguments>
                        <argument name="a"/>
                        <argument name="b" class="java.lang.Integer"/>
                    </arguments>
                </java>
            </invocation>
            <in>
                <field id="a" mapping="[0]"/>
                <field id="b" mapping="[1]"/>
            </in>
            <out>
                <field id="sum" mapping="#this"/>
            </out>
        </operation>
    </operations>

</object>
