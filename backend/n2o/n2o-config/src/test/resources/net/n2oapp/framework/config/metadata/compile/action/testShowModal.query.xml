<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0" object-id="testShowModal">
    <fields>
        <field id="id">
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="secondId">
            <filters>
                <eq filter-id="secondId"/>
            </filters>
        </field>
        <field id="name">
            <filters>
                <eq filter-id="name"/>
            </filters>
        </field>
        <field id="surname">
            <filters>
                <eq filter-id="surname"/>
            </filters>
        </field>
        <field id="birthday">
            <filters>
                <more filter-id="birthday.begin"/>
                <less filter-id="birthday.end"/>
            </filters>
        </field>
    </fields>
</query>