{
  "name": "n2o",
  "version": "7.3.3",
  "description": "N2O React",
  "jsnext:main": "es/index.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "dist",
    "lib",
    "es",
    "src/sass"
  ],
  "keywords": [
    "react",
    "redux",
    "bootstrap",
    "components",
    "ui",
    "javascript"
  ],
  "author": "I-Novus <n2o@i-novus.com>",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint ./src --ext .js,.jsx",
    "eslint:generate": "eslint ./src --ext .js,.jsx -o ./docs/eslint.json -f json",
    "prettier": "eslint ./src --ext .js,.jsx --fix",
    "esdoc": "./node_modules/.bin/esdoc",
    "stylelint": "stylelint \"components/**/*.css\" \"src/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "jest --noStackTrace --coverage",
    "test:watch": "jest --watch --noStackTrace",
    "test:hook": "jest --bail --findRelatedTests",
    "test:generate-output": "jest --json --outputFile=./.storybook/jest-test-results.json || echo 'jest: generate-output finished'",
    "start": "node scripts/run.js",
    "styles": "cpy src/less lib/less",
    "clean": "npm run clean:es && npm run clean:lib && npm run clean:dist",
    "clean:es": "rimraf ./es/*",
    "clean:lib": "rimraf ./lib/*",
    "clean:dist": "rimraf ./dist/*",
    "compile:es": "cross-env BABEL_ENV=es babel ./src -d es --ignore '*.test.js,*.story.js,*.scss,*.sass'",
    "compile:lib": "cross-env BABEL_ENV=commonjs babel ./src -d lib --ignore '*.test.js,*.story.js'",
    "compile:dist": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.build.js",
    "compile:es:watch": "cross-env BABEL_ENV=es ./node_modules/.bin/babel ./src -d es --ignore '*.test.js,*.story.js,*.scss,*.sass' --copy-files -w",
    "compile:lib:watch": "cross-env BABEL_ENV=commonjs ./node_modules/.bin/babel ./src -d lib --ignore '*.test.js,*.story.js' --copy-files -w",
    "compile:dist:watch": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.build.js -w",
    "build:es": "npm run clean:es && npm run compile:es",
    "build:lib": "npm run clean:lib && npm run compile:lib",
    "build:dist": "npm run clean:dist && npm run compile:dist",
    "build": "npm run build:es && npm run build:lib && npm run build:dist",
    "server": "node scripts/dev-server/dev-server.js",
    "build:local": "node scripts/build.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o ./docs/storybook",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "assets-webpack-plugin": "3.5.1",
    "axios": "0.18.0",
    "babel-polyfill": "6.23.0",
    "babel-runtime": "6.23.0",
    "bootstrap": "~4.3.1",
    "brace": "0.11.0",
    "classnames": "2.2.5",
    "connect-history-api-fallback": "1.3.0",
    "connected-react-router": "5.0.1",
    "copy-to-clipboard": "^3.2.0",
    "cpy-cli": "1.0.1",
    "deepmerge": "1.5.2",
    "draft-js": "0.10.5",
    "draftjs-to-html": "0.8.4",
    "flat": "4.1.0",
    "font-awesome": "4.7.0",
    "global": "4.3.2",
    "history": "4.6.1",
    "html-to-draftjs": "1.4.0",
    "lodash": "4.17.4",
    "moment": "2.20.1",
    "numeral": "2.0.6",
    "path-to-regexp": "2.2.1",
    "prop-types": "15.6.1",
    "query-string": "5.1.1",
    "rc-collapse": "~1.10.0",
    "rc-slider": "^8.6.11",
    "rc-switch": "~1.8.0",
    "rc-table": "6.4.3",
    "rc-tree-select": "2.3.1",
    "react-ace": "5.9.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-draft-wysiwyg": "1.13.1",
    "react-dropzone": "7.0.1",
    "react-helmet": "5.2.0",
    "react-hotkeys": "1.0.3",
    "react-intl": "2.8.0",
    "react-onclickoutside": "6.7.1",
    "react-popper": "^1.3.3",
    "react-redux": "5.0.6",
    "react-resizable": "1.7.5",
    "react-router": "4.3.1",
    "react-router-bootstrap": "0.24.4",
    "react-router-dom": "4.3.1",
    "react-rte": "0.16.0",
    "react-text-mask": "5.4.3",
    "react-textarea-autosize": "7.0.4",
    "react-virtualized": "^9.21.0",
    "reactstrap": "^8.0.0",
    "recompose": "0.25.0",
    "redux": "3.6.0",
    "redux-actions": "2.3.2",
    "redux-batched-actions": "0.3.0",
    "redux-devtools-extension": "2.13.2",
    "redux-form": "7.4.2",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.1",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "scroll-into-view-if-needed": "2.2.16",
    "storybook-react-router": "1.0.1",
    "text-mask-addons": "3.7.1",
    "whatwg-fetch": "2.0.3",
    "why-did-you-update": "0.1.1",
    "xhr-mock": "2.4.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@dump247/storybook-state": "1.3.0",
    "@storybook/addon-actions": "^3.3.12",
    "@storybook/addon-jest": "^3.3.13",
    "@storybook/addon-knobs": "^3.3.13",
    "@storybook/addon-links": "^3.3.12",
    "@storybook/addon-options": "^3.3.13",
    "@storybook/addons": "^3.3.12",
    "@storybook/react": "^3.3.12",
    "axios-mock-adapter": "^1.16.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "babel-standalone": "^6.26.0",
    "base64-js": "^1.2.1",
    "browser-sync": "^2.18.8",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.9",
    "ejs": "^2.5.6",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.0",
    "esdoc": "1.0.1",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "fetch-mock": "^6.0.0",
    "file-loader": "^1.1.6",
    "fs-extra": "^4.0.1",
    "http-proxy-middleware": "^0.17.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "20.0.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mocha": "^3.4.2",
    "node-sass": "^4.9.2",
    "prettier": "1.16.4",
    "react": "^16.0.0",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.0.3",
    "sinon": "^4.1.3",
    "storybook-addon-jsx": "^5.3.0",
    "style-loader": "^0.20.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs"
          ]
        ]
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single"
    }
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageReporters": [
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.story.{js,jsx}",
      "!src/**/*.test.{js,jsx}"
    ]
  }
}
