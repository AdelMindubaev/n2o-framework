{
  "List": {
    "src": "ListWidget",
    "className": "n2o",
    "dataProvider": {
      "url": "n2o/data/test",
      "pathMapping": {
      },
      "queryMapping": {}
    },
    "toolbar": {
      "topRight": [
        {
          "id": "topRight0",
          "buttons": [
            {
              "id": "menuItem0",
              "actionId": "menuItem0",
              "hint": "Изменить видимость фильтров",
              "icon": "fa fa-filter"
            },
            {
              "id": "menuItem1",
              "icon": "fa fa-table",
              "hint": "Изменить видимость колонок",
              "dropdownSrc": "ToggleColumn"
            },
            {
              "id": "menuItem2",
              "hint": "Обновить данные",
              "icon": "fa fa-refresh",
              "actionId": "menuItem2"
            },
            {
              "id": "menuItem3",
              "icon": "fa fa-bars",
              "hint": "Изменить размер",
              "dropdownSrc": "ChangeSize"
            },
            {
              "id": "menuItem4",
              "hint": "Экспортировать данные",
              "icon": "fa fa-download",
              "actionId": "exportModal"
            }
          ]
        }
      ]
    },
    "actions": {
      "menuItem0": {
        "src": "perform",
        "options": {
          "type": "n2o/widgets/TOGGLE_FILTER_VISIBILITY",
          "payload": {
            "widgetId": "Page_Table"
          }
        }
      },
      "menuItem2": {
        "src": "perform",
        "options": {
          "type": "n2o/widgets/DATA_REQUEST",
          "payload": {
            "widgetId": "Page_Table"
          }
        }
      },
      "exportModal": {
        "widgetId": "Page_Table",
        "src": "perform",
        "options": {
          "type": "n2o/modals/INSERT",
          "payload": {
            "widgetId": "Page_Table",
            "type": "n2o/modals/INSERT",
            "name": "test2",
            "src": "exportModal",
            "title": "Экспортровать",
            "size": "sm",
            "visible": true,
            "closeButton": false,
            "actions": {
              "export": {
                "src": "exportTable",
                "notification": {
                  "severity": "danger",
                  "text": "Это сообщение будет отображаться 10 секунд",
                  "timeout": 10000
                },
                "redirectTo": "/test",
                "options": {
                  "widgetId": "Page_Table"
                }
              },
              "destroyModal": {
                "src": "perform",
                "options": {
                  "type": "n2o/modals/DESTROY"
                }
              }
            },
            "toolbar": [
              {
                "buttons": [
                  {
                    "id": "export",
                    "title": "Загрузить",
                    "actionId": "export",
                    "className": "btn-primary",
                    "icon": "fa fa-download"
                  },
                  {
                    "id": "cancelExport",
                    "title": "Отменить",
                    "actionId": "destroyModal"
                  }
                ]
              }
            ]
          }
        }
      }
    },
    "paging": {
      "size": 10,
      "prev": true,
      "next": true
    },
    "filter": {
      "filterPlace": "top",
      "filterFieldsets": [
        {
          "src": "StandardFieldset",
          "rows": [
            {
              "cols": [
                {
                  "fields": [
                    {
                      "id": "testId",
                      "src": "StandardField",
                      "dependency": [],
                      "control": {
                        "id": "testId",
                        "src": "FilterButtonsField",
                        "searchLabel": "searchLabel",
                        "resetLabel": "resetLabel"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ],
      "filterButtonId": "filter",
      "blackResetList": [],
      "validation": {},
      "hideButtons": true
    },
    "hasMoreButton": true,
    "fetchOnScroll": false,
    "maxHeight": null,
    "maxWidth": null,
    "list": {
      "image": {
        "src": "ImageCell",
        "id": "image",
        "className": "list-image",
        "title": "It's a cat!"
      },
      "header": {
        "src": "BadgeCell",
        "id": "header"
      },
      "body": {
        "src": "TextCell",
        "id": "body",
        "visible": true
      },
      "subHeader": {
        "src": "TextCell",
        "id": "subHeader",
        "className": "list-subheader",
        "visible": true
      },
      "rightTop": {
        "src": "TextCell",
        "id": "rightTop",
        "visible": true
      },
      "rightBottom": {
        "src": "TextCell",
        "id": "rightBottom",
        "visible": true
      },
      "extra": {
        "src": "TextCell",
        "id": "extra",
        "visible": true
      }
    }
  }
}