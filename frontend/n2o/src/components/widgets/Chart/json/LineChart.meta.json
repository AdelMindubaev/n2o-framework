{
  "Page_Table": {
    "src": "ChartWidget",
    "dataProvider": {
      "url": "n2o/data/test",
      "pathMapping": {
      },
      "queryMapping": {
        "filter.name": {
          "link": "models.filter['Page_Table'].name"
        },
        "filter.surname": {
          "link": "models.filter['Page_Table'].surname"
        }
      }
    },
    "toolbar": {
      "topRight": [
        {
          "id": "topRight0",
          "buttons": [
            {
              "id": "menuItem0",
              "actionId": "menuItem0",
              "hint": "Изменить видимость фильтров",
              "icon": "fa fa-filter"
            },
            {
              "id": "menuItem1",
              "icon": "fa fa-table",
              "hint": "Изменить видимость колонок",
              "dropdownSrc": "ToggleColumn"
            },
            {
              "id": "menuItem2",
              "hint": "Обновить данные",
              "icon": "fa fa-refresh",
              "actionId": "menuItem2"
            },
            {
              "id": "menuItem3",
              "icon": "fa fa-bars",
              "hint": "Изменить размер",
              "dropdownSrc": "ChangeSize"
            },
            {
              "id": "menuItem4",
              "hint": "Экспортировать данные",
              "icon": "fa fa-download",
              "actionId": "exportModal"
            }
          ]
        }
      ]
    },
    "actions": {
      "menuItem0": {
        "src": "perform",
        "options": {
          "type": "n2o/widgets/TOGGLE_FILTER_VISIBILITY",
          "payload": {
            "widgetId": "Page_Table"
          }
        }
      },
      "menuItem2": {
        "src": "perform",
        "options": {
          "type": "n2o/widgets/DATA_REQUEST",
          "payload": {
            "widgetId": "Page_Table"
          }
        }
      },
      "exportModal": {
        "widgetId": "Page_Table",
        "src": "perform",
        "options": {
          "type": "n2o/modals/INSERT",
          "payload": {
            "widgetId": "Page_Table",
            "type": "n2o/modals/INSERT",
            "name": "test2",
            "src": "exportModal",
            "title": "Экспортровать",
            "size": "sm",
            "visible": true,
            "closeButton": false,
            "actions": {
              "export": {
                "src": "exportTable",
                "notification": {
                  "severity": "danger",
                  "text": "Это сообщение будет отображаться 10 секунд",
                  "timeout": 10000
                },
                "redirectTo": "/test",
                "options": {
                  "widgetId": "Page_Table"
                }
              },
              "destroyModal": {
                "src": "perform",
                "options": {
                  "type": "n2o/modals/DESTROY"
                }
              }
            },
            "toolbar": [
              {
                "buttons": [
                  {
                    "id": "export",
                    "title": "Загрузить",
                    "actionId": "export",
                    "className": "btn-primary",
                    "icon": "fa fa-download"
                  },
                  {
                    "id": "cancelExport",
                    "title": "Отменить",
                    "actionId": "destroyModal"
                  }
                ]
              }
            ]
          }
        }
      }
    },
    "chart": {
      "fetchOnInit": true,
      "type": "line",
      "layout": "horizontal",
      "width": 600,
      "height": 400,
      "margin": { "top": 5, "right": 5, "bottom": 5, "left": 5 },
      "XAxis": {
        "hide": false,
        "dataKey": "name",
        "width": 0,
        "height": 30,
        "orientation": "bottom",
        "type": "category",
        "allowDecimals": true,
        "allowDataOverflow": false,
        "allowDuplicatedCategory": true,
        "tickCount": 5,
        "interval": "preserveEnd",
        "padding": {
          "left": 0,
          "right": 0
        },
        "minTickGap": 5,
        "axisLine": true,
        "tickLine": true,
        "tickSize": 6,
        "label": false
      },
      "YAxis": {
        "hide": false,
        "dataKey": "name",
        "width": 0,
        "height": 30,
        "orientation": "left",
        "type": "number",
        "allowDecimals": true,
        "allowDataOverflow": false,
        "allowDuplicatedCategory": true,
        "tickCount": 5,
        "interval": "preserveEnd",
        "padding": {
          "left": 0,
          "right": 0
        },
        "minTickGap": 5,
        "axisLine": true,
        "tickLine": true,
        "tickSize": 6,
        "label": false
      },
      "cartesianGrid": {
        "x": 0,
        "y": 0,
        "width": 0,
        "height": 0,
        "horizontal": true,
        "vertical": true,
        "horizontalPoints": [],
        "verticalPoints": []
      },
      "tooltip": {
        "separator": ":",
        "offset": 10,
        "filterNull": true,
        "itemStyle": {},
        "wrapperStyle": {},
        "contentStyle": {},
        "labelStyle": {},
        "viewBox": {},
        "label": null
      },
      "legend": {
        "width": null,
        "height": null,
        "layout": "horizontal",
        "align": "center",
        "verticalAlign": "middle",
        "iconSize": 14,
        "iconType": "line",
        "margin": {},
        "wrapperStyle": {}
      },
      "lines": [
        {
          "type": "monotone",
          "dataKey": "pv",
          "stroke": "#8884d8",
          "legendType": "line",
          "dot": true,
          "activeDot": true,
          "label": false,
          "layout": "horizontal"
        },
        {
          "type": "monotone",
          "dataKey": "uv",
          "stroke": "#82ca9d",
          "legendType": "line",
          "dot": true,
          "activeDot": true,
          "label": false,
          "layout": "horizontal"
        }
      ]
    }
  }
}