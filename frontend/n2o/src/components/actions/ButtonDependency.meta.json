{
  "Page_Table": {
    "src": "TableWidget",
    "dataProvider": {
      "url": "n2o/data/test",
      "pathMapping": {
      },
      "queryMapping": {
      }
    },
    "toolbar": {
      "topRight": [
        {
          "buttons": [
            {
              "id": "filter",
              "actionId": "filter",
              "visible": true,
              "conditions": {
                "visible": [
                  {
                    "expression": "name !== 'Афанасий'",
                    "modelLink": "models.resolve['Page_Table']"
                  }
                ]
              },
              "title": "Видимость",
              "hint": "Скроется, если имя в выбранной записи Афанасий"
            },
            {
              "id": "enable",
              "actionId": "enable",
              "visible": true,
              "conditions": {
                "enabled": [
                  {
                    "expression": "name !== 'Иннокентий'",
                    "modelLink": "models.resolve['Page_Table']"
                  }
                ]
              },
              "title": "Блокировка",
              "hint": "Заблокируется, если имя в выбранной записи Иннокентий"
            },
            {
              "id": "combo",
              "actionId": "combo",
              "visible": true,
              "conditions": {
                "visible": [
                  {
                    "expression": "surname !== 'Ревин'",
                    "modelLink": "models.resolve['Page_Table']"
                  }
                ],
                "enabled": [
                  {
                    "expression": "name !== 'Людмила'",
                    "modelLink": "models.resolve['Page_Table']"
                  }
                ]
              },
              "title": "Комбинация",
              "hint": "Заблокируется, если имя в выбранной записи Людмила. Исчезнет, если фамилия Ревин"
            },
            {
              "id": "dropdown",
              "actionId": "dropdown",
              "title": "Видимость списка",
              "hint": "Исчезнет, если фамилия Ревин. Условие видимости на элементах списка",
              "subMenu": [
                {
                  "id": "dropdownItem1",
                  "visible": true,
                  "conditions": {
                    "visible": [
                      {
                        "expression": "surname !== 'Ревин'",
                        "modelLink": "models.resolve['Page_Table']"
                      }
                    ]
                  },
                  "title": "Элементы списка",
                  "hint": "Исчезнет, если фамилия Ревин"
                },
                {
                  "id": "dropdownItem2",
                  "visible": true,
                  "conditions": {
                    "visible": [
                      {
                        "expression": "surname !== 'Ревин'",
                        "modelLink": "models.resolve['Page_Table']"
                      }
                    ]
                  },
                  "title": "Элементы списка",
                  "hint": "Исчезнет, если фамилия Ревин"
                }
              ]
            }
          ]
        }
      ]
    },
    "actions": {
      "dummy": {
        "src": "dummy"
      }
    },
    "paging": false,
    "filter": {
    },
    "table": {
      "size": 10,
      "fetchOnInit": true,
      "hasSelect": true,
      "className": "n2o",
      "colorFieldId": "",
      "style": {},
      "hasFocus": true,
      "autoFocus": false,
      "sorting": {
        "birthday": "ASC"
      },
      "cells": [
        {
          "src": "TextCell",
          "id": "name"
        },
        {
          "src": "IconCell",
          "id": "surname",
          "icon": "fa fa-plus",
          "type": "iconAndText",
          "textPlace": "right"
        },
        {
          "src": "TextCell",
          "id": "birthday"
        }
      ],
      "headers": [
        {
          "src": "TextTableHeader",
          "id": "name",
          "sortable": false,
          "label": "Имя"
        },
        {
          "src": "TextTableHeader",
          "id": "surname",
          "sortable": true,
          "label": "Фамилия"
        },
        {
          "src": "TextTableHeader",
          "id": "birthday",
          "sortable": true,
          "label": "Дата рождения"
        }
      ]
    }
  }
}