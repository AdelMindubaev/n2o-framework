{
  "Page_Table": {
    "src": "TableWidget",
    "dataProvider": {
      "url": "n2o/data/test",
      "pathMapping": {
      },
      "queryMapping": {
      }
    },
    "toolbar": {
      "topRight": [
        {
          "buttons": [
            {
              "id": "create",
              "hint": "Добавить заптсь",
              "icon": "fa fa-plus",
              "actionId": "storyModal"
            }
          ]
        }
      ]
    },
    "actions": {
      "storyModal": {
        "widgetId": "Page_Table",
        "src": "perform",
        "options": {
          "type": "n2o/modals/INSERT",
          "payload": {
            "widgetId": "Page_Table",
            "type": "n2o/modals/INSERT",
            "name": "test2",
            "src": "storyModal",
            "title": "Добавить запись",
            "size": "sm",
            "visible": true,
            "closeButton": false,
            "actions": {
              "add": {
                "src": "perform",
                "options": {
                  "type": "n2o/actionImpl/START_INVOKE",
                  "payload": {
                    "widgetId": "Page_Table",
                    "modelLink": "form.storyForm.values"
                  },
                  "meta": {
                    "success": {
                      "refresh": true,
                      "closeLastModal": true,
                      "clearForm": "storyForm"
                    }
                  }
                }
              },
              "destroyModal": {
                "src": "perform",
                "options": {
                  "type": "n2o/modals/DESTROY"
                }
              }
            },
            "toolbar": [
              {
                "buttons": [
                  {
                    "id": "add",
                    "title": "Добавить",
                    "actionId": "add",
                    "color": "primary"
                  },
                  {
                    "id": "cancel",
                    "title": "Отменить",
                    "actionId": "destroyModal"
                  }
                ]
              }
            ]
          }
        }
      }
    },
    "paging": {
      "size": 10,
      "prev": true,
      "next": true
    },
    "filter": {
    },
    "table": {
      "size": 10,
      "fetchOnInit": true,
      "hasSelect": true,
      "className": "n2o",
      "colorFieldId": "",
      "style": {},
      "hasFocus": true,
      "autoFocus": false,
      "sorting": {
        "birthday": "ASC"
      },
      "cells": [
        {
          "src": "TextCell",
          "id": "name"
        },
        {
          "src": "IconCell",
          "id": "surname",
          "icon": "fa fa-plus",
          "type": "iconAndText",
          "textPlace": "right"
        },
        {
          "src": "TextCell",
          "id": "birthday"
        }
      ],
      "headers": [
        {
          "src": "TextTableHeader",
          "id": "name",
          "sortable": false,
          "label": "Имя"
        },
        {
          "src": "TextTableHeader",
          "id": "surname",
          "sortable": true,
          "label": "Фамилия"
        },
        {
          "src": "TextTableHeader",
          "id": "birthday",
          "sortable": true,
          "label": "Дата рождения"
        }
      ]
    }
  }
}