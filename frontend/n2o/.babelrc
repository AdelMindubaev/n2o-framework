{
  "presets": [
    "stage-1",
    "react"
  ],
  "plugins": [
    "transform-runtime"
  ],
  "env": {
    "commonjs": {
      "presets": [
        "es2015"
      ]
    },
    "es": {
      "presets": [
        ["es2015", {
          "modules": false
        }]
      ]
    },
    "test": {
      "plugins": [
        [
          "transform-es2015-modules-commonjs"
        ]
      ]
    }
  }
}