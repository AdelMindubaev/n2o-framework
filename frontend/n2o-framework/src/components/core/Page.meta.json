{
  "id": "testSimplePageJson",
  "page": {
    "title": "`'Вторая: ' + id`",
    "modelLink": "pages['testSimplePageJson'].metadata"
  },
  "routes": {
    "list": [
      {
        "path" : "/test",
        "exact": true
      },
      {
        "path" : "/test/wireframe",
        "exact": true
      },
      {
        "path" : "/test/wireframe/:wireframe_id",
        "exact": true
      }
    ]
  },
  "breadcrumb": [
    {
      "label": "Первая",
      "path": "/"
    },
    {
      "label": "Вторая",
      "path": "/test"
    }
  ],
  "widgets": {
    "Page_Wireframe": {
      "src": "WireframeWidget",
      "name": "name",
      "toolbar": {
        "topLeft": [
          {
            "buttons": [
              {
                "id": "redirect",
                "src": "PerformButton",
                "label": "Вперед"
              }
            ]
          }
        ]
      },
      "actions": {
        "redirect": {
          "src": "link",
          "options": {
            "target": "application",
            "path": "/test"
          }
        }
      },
      "wireframe": {
        "className": "",
        "title": "Виджет первой страницы",
        "height": 300,
        "fetchOnInit": false
      }
    }
  },
  "layout": {
    "src": "SingleLayout",
    "regions": {
      "single": [
        {
          "src": "NoneRegion",
          "items": [
            {
              "widgetId": "Page_Wireframe"
            }
          ]
        }
      ]
    }
  }
}