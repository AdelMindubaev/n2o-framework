<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-4.0">
    <operations>
        <operation id="newProcess">
            <invocation>
                <camunda operation="startProcess"/>
            </invocation>
            <in>
                <field id="id" domain="string" required="true"/>
                <field id="position" domain="string" required="true"/>
                <field id="salary" domain="string" required="true"/>
            </in>
        </operation>
    </operations>
</object>