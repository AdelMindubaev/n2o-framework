<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Добавление файла">
    <form>
        <datasource object-id="file"/>
        <fields>
            <file-upload id="fileUpload" multi="true" no-label="true"
                         upload-url="/files" delete-url="/files"
                         value-field-id="id" label-field-id="fileName" url-field-id="url">
            </file-upload>
        </fields>
    </form>
</simple-page>