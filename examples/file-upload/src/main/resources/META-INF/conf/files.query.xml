<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0"
       object-id="file">
    <list result-mapping="#this" count-mapping="#this.length">
        <java method="getList">
            <spring bean="fileStorageController" />
        </java>
    </list>

    <fields>
        <field id="id">
            <select/>
        </field>
        <field id="fileName">
            <select/>
        </field>
        <field id="url">
            <select/>
        </field>
    </fields>
</query>