<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="META-INF/conf/customization.json"></test>
    </list>
    <count>
        <test file="META-INF/conf/customization.json" operation="count"></test>
    </count>
    <fields>
        <field id="id">
            <select/>
        </field>
        <field id="name">
            <select/>
        </field>
        <field id="engine" domain="numeric">
            <select/>
        </field>
        <field id="power" domain="integer">
            <select/>
        </field>
    </fields>
</query>
