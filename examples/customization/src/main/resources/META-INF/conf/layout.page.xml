<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-2.0" name="Кастомный макет SquareLayout" layout="SquareLayout">
    <regions>
        <panel place="topLeft">
            <table query-id="test" size="5" name="Положение региона topLeft">
                <columns>
                    <column text-field-id="id"></column>
                    <column text-field-id="name"></column>
                </columns>
            </table>
        </panel>
        <panel place="topRight">
            <form upload="defaults" name="Положение региона topRight">
                <fields>
                    <input-text enabled="false" id="Place" default-value="topRight"/>
                </fields>
            </form>
        </panel>
        <panel place="bottomLeft">
            <form upload="defaults" name="Положение региона bottomLeft">
                <fields>
                    <input-text enabled="false" id="Place" label="" default-value="bottomLeft"/>
                </fields>
            </form>
        </panel>
        <panel place="bottomRight" >
            <table query-id="test" size="5" name="Положение региона bottomRight">
                <columns>
                    <column text-field-id="id"></column>
                    <column text-field-id="name"></column>
                </columns>
            </table>
        </panel>
    </regions>
</page>